{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Used for validation of resource.json JSON file.",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "geoname-id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "name-full": {
                    "type": "string"
                },
                "updated-at": {
                    "$ref": "#/$defs/updatedAt"
                },
                "properties": {
                    "$ref": "#/$defs/properties"
                },
                "feature": {
                    "$ref": "#/$defs/feature"
                },
                "coordinate": {
                    "$ref": "#/$defs/coordinate"
                },
                "timezone": {
                    "$ref": "#/$defs/timezone"
                },
                "locations": {
                    "$ref": "#/$defs/locations"
                },
                "next-places": {
                    "$ref": "#/$defs/nextPlacesWithConfig"
                },
                "links": {
                    "$ref": "#/$defs/links"
                },
                "next-places-config": {
                    "$ref": "#/$defs/nextPlacesConfig"
                }
            },
            "required": [
                "geoname-id",
                "name",
                "updated-at",
                "feature",
                "coordinate",
                "timezone",
                "locations",
                "links",
                "next-places-config"
            ],
            "additionalProperties": false
        },
        "given": {
            "$ref": "#/$defs/given"
        },
        "time-taken": {
            "type": "string"
        },
        "memory-taken": {
            "type": "string"
        },
        "version": {
            "type": "string"
        },
        "data-licence": {
            "$ref": "#/$defs/dataLicense"
        }
    },
    "required": ["data", "given", "time-taken", "memory-taken", "version", "data-licence"],
    "additionalProperties": false,



    "$defs": {
        "class": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "string",
            "enum": [
                "A",
                "H",
                "L",
                "P",
                "R",
                "S",
                "T",
                "U",
                "V"
            ]
        },
        "code": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "string",
            "pattern": "^[A-Z]+[0-9]?$"
        },
        "coordinateValue": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "decimal": {
                    "type": "number"
                },
                "dms": {
                    "type": "string"
                }
            },
            "required": [
                "decimal",
                "dms"
            ],
            "additionalProperties": false
        },
        "locationsSimple": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "district-locality": {
                    "type": "string"
                },
                "borough-locality": {
                    "type": "string"
                },
                "city-municipality": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                }
            },
            "required": [],
            "additionalProperties": false
        },
        "nextPlaces": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "patternProperties": {
                "^[AHLPRSTUV]$": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "link": {
                                "type": "string",
                                "pattern": "^https?://.*$"
                            },
                            "number": {
                                "type": "integer",
                                "minimum": 0
                            }
                        },
                        "required": [
                            "link",
                            "number"
                        ],
                        "additionalProperties": false
                    }
                }
            },
            "additionalProperties": false
        },
        "nextPlacesWithConfig": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "patternProperties": {
                "^[AHLPRSTUV]$": {
                    "type": "object",
                    "properties": {
                        "config": {
                            "type": "object",
                            "properties": {
                                "distance-meter": {
                                    "type": "integer",
                                    "minimum": 0
                                },
                                "limit": {
                                    "type": "integer",
                                    "minimum": 0
                                }
                            },
                            "required": [
                                "distance-meter",
                                "limit"
                            ],
                            "additionalProperties": false
                        },
                        "feature": {
                            "type": "object",
                            "properties": {
                                "class": {
                                    "$ref": "#/$defs/class"
                                },
                                "class-name": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "class",
                                "class-name"
                            ],
                            "additionalProperties": false
                        },
                        "places": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "geoname-id": {
                                        "type": "integer"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "updated-at": {
                                        "$ref": "#/$defs/updatedAt"
                                    },
                                    "coordinate": {
                                        "$ref": "#/$defs/coordinate"
                                    },
                                    "feature": {
                                        "type": "object",
                                        "properties": {
                                            "code": {
                                                "$ref": "#/$defs/code"
                                            },
                                            "code-name": {
                                                "type": "string"
                                            }
                                        },
                                        "required": [
                                            "code",
                                            "code-name"
                                        ],
                                        "additionalProperties": false
                                    },
                                    "links": {
                                        "$ref": "#/$defs/links"
                                    },
                                    "properties": {
                                        "$ref": "#/$defs/properties"
                                    }
                                },
                                "required": [
                                    "geoname-id",
                                    "name",
                                    "updated-at",
                                    "coordinate"
                                ],
                                "additionalProperties": false
                            }
                        }
                    },
                    "required": ["config", "feature"],
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        },
        "nextPlacesConfig": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "config": {
                    "patternProperties": {
                        "^.*$": {
                            "type": "object",
                            "properties": {
                                "distance": {
                                    "type": "integer",
                                    "minimum": 0
                                },
                                "limit": {
                                    "type": "integer",
                                    "minimum": 0
                                },
                                "name": {
                                    "type": "string"
                                },
                                "feature_codes": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            },
                            "required": [
                                "distance",
                                "limit",
                                "name",
                                "feature_codes"
                            ],
                            "additionalProperties": false
                        }
                    }
                },
                "endpoints": {
                    "type": "object",
                    "properties": {
                        "coordinate": {
                            "type": "string"
                        },
                        "list": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "coordinate",
                        "list"
                    ],
                    "additionalProperties": false
                }
            },
            "required": [
                "config",
                "endpoints"
            ],
            "additionalProperties": false
        },
        "valueUnitFormatted": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number"
                },
                "unit": {
                    "type": "string"
                },
                "value-formatted": {
                    "type": "string"
                }
            },
            "required": [
                "value",
                "unit",
                "value-formatted"
            ],
            "additionalProperties": false
        },
        "updatedAt": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}\\+[0-9]{2}:[0-9]{2}$"
        },



        "coordinate": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "latitude": {
                    "$ref": "#/$defs/coordinateValue"
                },
                "longitude": {
                    "$ref": "#/$defs/coordinateValue"
                },
                "srid": {
                    "type": "number"
                },
                "distance": {
                    "type": "object",
                    "properties": {
                        "meters": {
                            "$ref": "#/$defs/valueUnitFormatted"
                        },
                        "kilometers": {
                            "$ref": "#/$defs/valueUnitFormatted"
                        }
                    },
                    "required": [
                        "meters",
                        "kilometers"
                    ],
                    "additionalProperties": false
                },
                "direction": {
                    "type": "object",
                    "properties": {
                        "degrees": {
                            "type": "number"
                        },
                        "direction": {
                            "type": "string",
                            "enum": ["N", "NE", "E", "SE", "S", "SW", "W", "NW"]
                        }
                    }
                }
            },
            "required": [
                "latitude",
                "longitude",
                "srid"
            ],
            "additionalProperties": false
        },
        "dataLicense": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "full": {
                    "type": "string"
                },
                "short": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "full",
                "short",
                "url"
            ]
        },
        "feature": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "class": {
                    "$ref": "#/$defs/class"
                },
                "class-name": {
                    "type": "string"
                },
                "code": {
                    "$ref": "#/$defs/code"
                },
                "code-name": {
                    "type": "string"
                }
            },
            "required": ["class", "class-name", "code", "code-name"],
            "additionalProperties": false
        },
        "given": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "coordinate": {
                    "type": "object",
                    "properties": {
                        "raw": {
                            "type": "string"
                        },
                        "parsed": {
                            "type": "object",
                            "properties": {
                                "latitude": {
                                    "$ref": "#/$defs/coordinateValue"
                                },
                                "longitude": {
                                    "$ref": "#/$defs/coordinateValue"
                                }
                            },
                            "required": ["latitude", "longitude"],
                            "additionalProperties": false
                        }
                    },
                    "required": ["raw", "parsed"],
                    "additionalProperties": false
                },
                "language": {
                    "type": "object",
                    "properties": {
                        "raw": {
                            "type": "string"
                        },
                        "parsed": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                }
                            },
                            "additionalProperties": false,
                            "required": ["name"]
                        }
                    },
                    "additionalProperties": false,
                    "required": ["raw", "parsed"]
                }
            },
            "required": ["coordinate"],
            "additionalProperties": false
        },
        "links": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "wikipedia": {
                    "type": "object",
                    "properties": {
                        "this": {
                            "type": "string",
                            "pattern": "^https?://.*$"
                        },
                        "locations": {
                            "$ref": "#/$defs/locationsSimple"
                        },
                        "next-places": {
                            "$ref": "#/$defs/nextPlaces"
                        }
                    },
                    "required": [],
                    "additionalProperties": false
                },
                "maps": {
                    "type": "object",
                    "properties": {
                        "google": {
                            "type": "string",
                            "pattern": "^https?://.*$"
                        },
                        "openstreetmap": {
                            "type": "string",
                            "pattern": "^https?://.*$"
                        }
                    },
                    "required": [
                        "google",
                        "openstreetmap"
                    ],
                    "additionalProperties": false
                }
            },
            "required": [
                "maps"
            ],
            "additionalProperties": false
        },
        "locations": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "district-locality": {
                    "$ref": "#/$defs/location"
                },
                "borough-locality": {
                    "$ref": "#/$defs/location"
                },
                "city-municipality": {
                    "$ref": "#/$defs/location"
                },
                "state": {
                    "$ref": "#/$defs/location"
                },
                "country": {
                    "$ref": "#/$defs/location"
                }
            },
            "required": [
                "state",
                "country"
            ],
            "additionalProperties": false
        },
        "location": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": ["object", "null"],
            "properties": {
                "geoname-id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "updated-at": {
                    "$ref": "#/$defs/updatedAt"
                },
                "coordinate": {
                    "$ref": "#/$defs/coordinate"
                },
                "feature": {
                    "$ref": "#/$defs/feature"
                },
                "links": {
                    "$ref": "#/$defs/links"
                },
                "next-places-config": {
                    "$ref": "#/$defs/nextPlacesConfig"
                },
                "properties": {
                    "$ref": "#/$defs/properties"
                }
            },
            "required": [
                "geoname-id",
                "name",
                "updated-at",
                "coordinate",
                "feature",
                "links",
                "next-places-config",
                "properties"
            ],
            "additionalProperties": false
        },
        "properties": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "population": {
                    "$ref": "#/$defs/valueUnitFormatted"
                },
                "elevation": {
                    "$ref": "#/$defs/valueUnitFormatted"
                },
                "country": {
                    "type": "string"
                }
            },
            "required": [],
            "additionalProperties": false
        },
        "timezone": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "type": "object",
            "properties": {
                "timezone": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "current-time": {
                    "type": "object",
                    "properties": {
                        "timezone": {
                            "type": "string"
                        },
                        "utc": {
                            "type": "string"
                        }
                    },
                    "required": ["timezone", "utc"],
                    "additionalProperties": false
                },
                "offset": {
                    "type": "string",
                    "pattern": "^[+-]([0-9]{2}):([0-9]{2})$"
                },
                "coordinate": {
                    "$ref": "#/$defs/coordinate"
                }
            },
            "required": [
                "timezone",
                "country",
                "current-time",
                "offset"
            ],
            "additionalProperties": false
        }
    }
}